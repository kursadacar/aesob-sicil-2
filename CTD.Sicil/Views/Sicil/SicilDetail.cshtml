                        data: model.Ilces
                    });

                    $("[data-attribute='MerkezId']").val(model.Id);
                    $("[data-attribute='SicilId']").val(model.SicilId);
                    $("[data-attribute='NaceKodu']").val(model.NaceId);
                    $("[data-attribute='Meslek']").val(model.MeslekId);
                    $("[data-attribute='MeslekOdasi']").val(model.MeslekOdasi);
                    $("[data-attribute='Sinif']").val(model.Sinif);

                    var kayittarih = model.KayitTar;
                    var vizetarih = model.VizeSuresiBitTar;
                    var d, d2;
                    if (kayittarih == null) {
                        $("[data-attribute='KayitTar']").val();
                    }
                    else {
                        d = new Date(tarihformati(kayittarih));
                        $("[data-attribute='KayitTar']").val(d.getDate() + "." + (d.getMonth() + 1) + "." + d.getFullYear());
                    }
                    if (vizetarih == null) {
                        $("#newsubevizetarihi").val();
                    }
                    else {
                        d2 = new Date(tarihformati(vizetarih));
                        $("#newsubevizetarihi").val(d2.getDate() + "." + (d2.getMonth() + 1) + "." + d2.getFullYear());
                    }

                }
            });
        }
