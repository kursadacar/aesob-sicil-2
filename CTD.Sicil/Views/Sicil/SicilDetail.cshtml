                    </button>
                    <span class="modal-title f-16">YENİ MESLEK İLAVESİ</span>
                </div>
                <div class="modal-body" data-id="formYeniMeslekEkle" data-model-url="/Sicil/YeniMeslekEkle" data-ajax-function="funcDonenMeslekEkle">
                    <div class="form-group text-center">
                        <span class="modal-title font-16">@ViewBag.Title</span>
                    </div>
                    <div class="col-md-3">
                        <div class="input-group">
                            <label>NACE KODU</label> <input type="text" id="newnacekodu" name="newnacekodu" class="form-control" style="background-color: #f8fafb;" /> <input type="hidden" data-attribute="newnacekoduid" /> <input type="hidden" id="newsicilid" data-attribute="newsicilid" />
                        </div>
                    </div>
                    <div class="col-md-9">
                        <label>MESLEK KODU ve ADI</label><br /> <span id="spNewMeslekKoduAdi"></span> <input type="hidden" data-attribute="newmeslekno" />
                    </div>
                    <div style="clear: both"></div>
                    <div class="col-md-12 m-t-5 m-b-10"> <label>NACE TANIMI:</label>&nbsp;<span id="spNewNaceTanim"></span> </div>
                    <div class="clearfix"></div>
                    <div class="col-md-7">
                        <div class="form-group">
                            <label>MESLEK ODASI</label> <input class="form-control" data-attribute="newmeslekodasi" />
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="form-group">
                            <label>SINIFI</label> <input id="newsinif" data-attribute="newsinif" class="form-control" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-md-12">
                        <div class="input-group">
                            <label>İŞYERİ ÜNVANI</label> <input id="newunvan" type="text" data-attribute="newunvan" class="form-control" onkeyup="BuyukHarf(this)" style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-md-4">
                        <div class="form-group">
                            <label>İŞ ADRESİ İLÇE</label> <input id="newilce" data-attribute="newilce" class="form-control" onchange="funcIlceChange3(this)" />
                        </div>
                    </div>
                    <div class="col-md-8 col-xs-6 col-sm-6">
                        <div class="form-group">
                            <label>İŞ ADRESİ MAHALLE</label> <input id="newmahalle" data-attribute="newmahalle" class="form-control" onchange="funcMahalleChange3(this)" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>BULVAR/CADDE/SOKAK</label> <input id="newcadde" data-attribute="newcadde" class="form-control" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>APARTMAN/BİNA/NO</label> <input type="text" id="newadres" data-attribute="newadres" class="form-control" onkeyup="BuyukHarf(this)" style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-md-4">
                        <div class="form-group">
                            <label>&nbsp;KAYIT TARİHİ</label> <input type="text" id="newkayittarihi2" data-attribute="newkayittarihi2" class="form-control" onkeyup="tarih(this);" style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="col-md-4 col-xs-6 col-sm-6">
                        <div class="form-group">
                            <label>&nbsp;SON GEÇERLİLİK TARİHİ</label> <input type="text" id="newvizetarihi2" data-attribute="newvizetarihi2" class="form-control" onkeyup="tarih(this);" style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnNewProfessionCancel" class="btn btn-danger" data-dismiss="modal">İptal</button><a href="javascript:void(formSendModel($('[data-id=\'formYeniMeslekEkle\']')[0].dataset));" class="btn btn-lg btn-primary"><i class="fa fa-save"></i> &nbsp; Meslek Kaydet</a>
                </div>
            </div>
        </div>
    </div>
    <div class="modal inmodal" id="myModalSgk" tabindex="-1" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content animated bounceInDown">
                <div class="modal-header">
                    <button type="button" id="btnSgkClose" class="close" data-dismiss="modal">
                        <span aria-hidden="true">&times;</span><span class="sr-only">Kapat</span>
                    </button> <span class="modal-title">SGK YAZIŞMA</span>
                </div>
                <div class="modal-body">
                    <div class="tabs-container">
                        <form id="sgkdevamform">
                            <div class="col-md-12">
                                <div class="col-md-6">@Html.RadioButton("rbGrp", "1", true) @Html.Label("Kaydı Var")</div>
                                <div class="col-md-6">@Html.RadioButton("rbGrp", "2", false) @Html.Label("Kaydı Yok")</div> <hr />
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-md-3">
                                <div class="input-group">
                                    <label for="sgksayi">SAYI</label> <input id="sgksayi" type="text" data-attribute="sgksayi" class="form-control" onkeyup="BuyukHarf(this)" style="background-color: #f8fafb;" />
                                </div>
                            </div>
                            <div class="col-md-4 col-xs-6 col-sm-6">
                                <div class="form-group">
                                    <label for="sgktarih">TARİH</label> <input type="date" id="sgktarih" data-attribute="sgktarih" class="form-control" onkeyup="tarih(this);" style="background-color: #f8fafb;" />
                                </div>
                            </div>
                            <div class="col-md-5">
                                <div class="input-group">
                                    <label for="sgkkonu">KONUSU</label> <input id="sgkkonu" type="text" data-attribute="sgkkonu" class="form-control" value="Sicil Kaydı İncelemesi" placeholder="Evrak Konusu" style="background-color: #f8fafb;" />
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-md-12">
                                <div class="input-group">
                                    <input id="sgkanakurum" type="text" data-attribute="sgkanakurum" class="form-control" value="ANTALYA SOSYAL GÜVENLİK İL MÜDÜRLÜĞÜ" style="background-color: #f8fafb;" />
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-md-12">
                                <div class="input-group">
                                    <input id="sgkaltkurum" type="text" data-attribute="sgkaltkurum" class="form-control" value="_________ Sosyal Güvenlik Merkezi" style="background-color: #f8fafb;" />
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-md-12">
                                <div class="input-group">
                                    <input id="sgkilgi" type="text" data-attribute="sgkilgi" class="form-control" value="_________ tarih ve _________ sayılı yazınız." style="background-color: #f8fafb;" />
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-md-12">
                                <div class="input-group">
                                    <textarea class="form-control" cols="200" id="sgkmetin" name="sgkdevammetin" rows="4" style="background-color: #f8fafb;"></textarea>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </form>
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="hidden" id="newsicilid2" value="@Model.Sicil.Id" /> <a href="#" class="btn btn-danger" onclick="location.href = '/Sicil/Report?id=PDF' + '&sayi=' + document.getElementById('sgksayi').value + '&tarih=' + document.getElementById('sgktarih').value + '&konu=' + document.getElementById('sgkkonu').value + '&anakurum=' + document.getElementById('sgkanakurum').value + '&altkurum=' + document.getElementById('sgkaltkurum').value + '&ilgi=' + document.getElementById('sgkilgi').value + '&metin=' + document.getElementById('sgkmetin').value + '&sicilid=' + document.getElementById('newsicilid2').value + '&durum=' + $('input[name=rbGrp]:checked').val();"><i class="fa fa-file-pdf-o"></i> Yazdır (.pdf)</a> <a href="#" class="btn btn-info" onclick="location.href = '/Sicil/Report?id=Word' + '&sayi=' + document.getElementById('sgksayi').value + '&tarih=' + document.getElementById('sgktarih').value + '&konu=' + document.getElementById('sgkkonu').value + '&anakurum=' + document.getElementById('sgkanakurum').value + '&altkurum=' + document.getElementById('sgkaltkurum').value + '&ilgi=' + document.getElementById('sgkilgi').value + '&metin=' + document.getElementById('sgkmetin').value + '&sicilid=' + document.getElementById('newsicilid2').value;"><i class="fa fa-file-word-o"></i> Yazdır (.doc)</a>
                </div>
            </div>
        </div>
    </div>

