                        placeholder: 'Bulvar Cadde Sokak seçiniz',
                    });
                });
            $("[data-attribute='IsAdresCadSokBulv']").select2('open');
        }

        function updateAddress() {
            let ilce = $("[data-attribute='ISADRESILCE']").select2('data').text;
            let mahalle = $("[data-attribute='ISADRESMAHALLE']").select2('data').text;
            let cadSokBulv = $("[data-attribute='ISADRESCADSOKBULV']").select2('data').text;
            let adres = $("[data-attribute='ISADRES']").val();

            let tumAdres = mahalle + " " + cadSokBulv + " " + adres + " - " + ilce + " / ANTALYA";

            $("[data-attribute='ISADRES2']").val(tumAdres);
            $("#lblAdresTum").text(tumAdres);
        }

        function funcIlceChange3() {
            var id = $("[data-attribute='newilce']").val();
