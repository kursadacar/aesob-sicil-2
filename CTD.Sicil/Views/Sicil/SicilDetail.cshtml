        function SubeEkle(id) {
            $("#newsubeunvan").focus();
            $("[data-attirbute='IsAdresMahalle']").select2({
                placeholder: 'Mahalle seçiniz'
            });
            $("[data-attirbute='IsAdresCadSokBulv']").select2();
