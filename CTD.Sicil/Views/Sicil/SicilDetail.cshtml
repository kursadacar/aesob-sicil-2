    Layout = "~/Views/Shared/_Layout.cshtml";
    var ileri = int.Parse(Model.Sicil.SICILNO.ToString()) + 1;
    var geri = int.Parse(Model.Sicil.SICILNO.ToString()) - 1;
}
<style>
    .panel-body {
        padding: 0;
    }
</style>
<link href="/Scripts/select2/select2.css" rel="stylesheet" />

@if (Accesses.Hak != "muhasebe")
{
