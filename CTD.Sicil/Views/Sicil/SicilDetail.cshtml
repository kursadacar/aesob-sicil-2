        function bugununtarihi() {
            var bugun = null;
            var tarih = new Date();
            var gun = tarih.getDay();
            var ay = tarih.getMonth();
            var yil = tarih.getFullYear();
            var gunler = ['Pazar', 'Pazartesi', 'Salı', 'Çarşamba', 'Perşembe', 'Cuma', 'Cumartesi'];
            var aylar = ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12'];
            bugun = tarih.getDate() + '.' + aylar[ay] + '.' + yil
            return bugun;
        }
        function vizetarihi() {
            var bugun = null;
            var tarih = new Date();
            var gun = tarih.getDay();
            var ay = tarih.getMonth();
            var vizeyil = tarih.getFullYear() + 5;
            var gunler = ['Pazar', 'Pazartesi', 'Salı', 'Çarşamba', 'Perşembe', 'Cuma', 'Cumartesi'];
            var aylar = ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12'];
            bugun = tarih.getDate() + '.' + aylar[ay] + '.' + vizeyil;
            return bugun;
        }

        function SgkForm(id) {
            var d = new Date();
            var y = d.getFullYear();
            var m = d.getMonth() + 1;
            var g = d.getDate();

            if (m < 10) m = "0" + m;
            if (g < 10) g = "0" + g;
            var today = y + "-" + m + "-" + g;

            $("#sgktarih").attr("value", today);
            $("#sgksayi").val(y + "-");

            $.ajax({
                url: "/Sicil/SicilSgkBilgi/",
                data: { id: id },
                datatype: "json",
                success: function (e) {
                    var dogumum = e.DogumTarihi;
                    var ddogum = new Date(tarihformati(dogumum));
                    var dogumtarihi = ddogum.getDate() + "." + (ddogum.getMonth() + 1) + "." + ddogum.getFullYear();
                    if (e.Durum === 0) {
                        $("#sgkmetin").val("Esnaf Sicil Müdürlüğümüzdeki kayıtların incelenmesi sonucunda, " + e.BabaAdi + " oğlu/kızı " + dogumtarihi + " doğumlu " + e.AdSoyad + "" + e.TcKimlikNo + ", müdürlüğümüzde " + e.SicilNo + " sicil no.su ile kayıtlı iken kaydını sildirmiştir. Kişiye ait kayıt ve terk durumları aşağıda çıkartılmıştır.");
                    }
                    else if (e.Durum > 0) {
                        $("#sgkmetin").val("Esnaf Sicil Müdürlüğümüzdeki kayıtların incelenmesi sonucunda, " + e.BabaAdi + " oğlu/kızı " + dogumtarihi + " doğumlu " + e.AdSoyad + "" + e.TcKimlikNo + ", müdürlüğümüzde " + e.SicilNo + " sicil no.su ile kaydı devam etmekte olup, kişiye ait kayıt ve terk durumları aşağıda çıkartılmıştır.");
                    }
                }
            });
        }

