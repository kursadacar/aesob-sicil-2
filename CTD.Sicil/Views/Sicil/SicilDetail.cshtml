                            <label>İŞ ADRESİ MAHALLE</label> <input class="form-control" data-attribute="IsAdresMahalle" onchange="funcMahalleChange2(this)" />
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="form-group">
                            <label>BULVAR/CADDE/SOKAK</label> <input class="form-control" data-attribute="IsAdresCadSokBulv" />
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-md-6">
                        <div class="input-group">
                            <label>APARTMAN/BİNA/NO</label> <input type="text" class="form-control" data-attribute="IsAdres" onkeyup="BuyukHarf(this);" placeholder="Apartman/Bina/No giriniz." style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>&nbsp;KAYIT TARİHİ</label> <input type="text" id="newsubekayittarihi" data-attribute="KayitTar" class="form-control" onkeyup="tarih(this);" style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="col-md-3 col-xs-6 col-sm-6">
                        <div class="form-group">
                            <label>&nbsp;SON GEÇERLİLİK TARİHİ</label> <input type="text" id="newsubevizetarihi" data-attribute="VizeSuresiBitTar" class="form-control" onkeyup="tarih(this);" style="background-color: #f8fafb;" />
                        </div>
                    </div>
                    <div class="clearfix"></div> <input type="hidden" data-attribute="MerkezId" /> <input type="hidden" data-attribute="SicilId" /> <input type="hidden" data-attribute="NaceKodu" /> <input type="hidden" data-attribute="MeslekOdasi" /> <input type="hidden" data-attribute="Meslek" /> <input type="hidden" data-attribute="Sinif" />
                </div>
                <div class="modal-footer">
                    <button type="button" id="btnNewSubeCancel" class="btn btn-danger" data-dismiss="modal">İptal</button> <a href="javascript:void(formSendModel($('[data-id=\'formYeniSubeEkle\']')[0].dataset));" class="btn btn-lg btn-primary"> <i class="fa fa-save"></i> &nbsp;&nbsp; Şube Kaydet </a>
                </div>
            </div>
        </div>
    </div>
    <div class="modal inmodal" id="myModalNewProfession" tabindex="-1" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content animated bounceInDown">
                <div class="modal-header">
                    <button type="button" id="btnNewMeslekClose" class="close" data-dismiss="modal">
