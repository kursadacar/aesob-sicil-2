                            <div class="form-group">
                                <label>MESLEK TERK NEDENİ</label> <input class="form-control" data-attribute="MESLEKTERKNEDENI" />
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-md-12">
                            <div class="input-group">
                                <label>AÇIKLAMA</label> <input id="aciklama" type="text" name="aciklama" data-attribute="ACIKLAMA" class="form-control" style="background-color: #f8fafb;" />
