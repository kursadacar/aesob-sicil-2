                datatype: "json",
                type: "GET",
                success: function (model) {
                    $("#lblSubeOdasi").text(model.MeslekOdasiString + " - " + model.Sinif + ".SINIF");
                    $("#lblSubeNaceTanimi").text(model.NaceKoduString + " - " + model.NaceTanimi);
                    $("#lblSubeMeslekKodu").text(model.MeslekKodu + " - " + model.MeslekTanimi);

                    $("[data-attribute='IsAdresIlce']").select2({
                        placeholder: 'İlçe seçiniz',
