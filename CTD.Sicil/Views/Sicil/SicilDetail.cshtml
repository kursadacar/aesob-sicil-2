@section scripts
{
    <script src="/Scripts/select2/select2.min.js"></script>

    <script>
        $("#dvYapilanIslemler").hide();
        if ($("#durum").val() == 1) {
            $("#anabaslik").addClass("bg-green");
        } else {
            $("#anabaslik").addClass("bg-red");
        }
        $("[data-attribute='Sicil.CINSIYET']").select2();
        if ($("#hdCinsiyet").val() == 1) {
            $("[data-attribute='Sicil.CINSIYET']").select2("val", "1");
        } else {
            $("[data-attribute='Sicil.CINSIYET']").select2("val", "2");
        }

        $("tr").on("click",
            function() {
                $("tr").removeClass("selected");
                $(this).addClass("selected");
            });

        function MeslekDetay(id) {
            $('#tbMeslekLog').empty();

            $(this).addClass("selected");

            $("#spTerkTarihi").empty();
            $("#spUnvan").empty();
            $("#spSicilId").empty();
            $("#spIsAdres").empty();
            $("#spOda").empty();
            $("#spSinif").empty();
            $("#spNace").empty();
            $("#spNaceTanimi").empty();
            $("#spKayitTarihi").empty();
            $("#spMeslekKodu").empty();
            $("#spMeslekAdim").empty();
            $("#spVizeBitis").empty();
            $("#spSonDegTrh").empty();
            $("#spSonVizeIslemTrh").empty();
            $("#spTerkNedeni").empty();
            $("#spMeslekAktarim").empty();
            $("#spAciklama").empty();

            $.ErcanAyhan.Server.GetAsync("",
                "/Sicil/SicilDetailMeslek?id=" + id,
                function(e) {
                    document.getElementById("dvMeslek").style.visibility = "visible";
                    $("#spUnvan").text(e.IsyeriUnvani);
                    $("#spSicilId").text(e.SicilId);
                    $("#spIsAdres").text(e.IsAdres2);
                    $("#spOda").text(e.OdaKisaAd);
                    $("#spSinif").text(e.SinifString);
                    $("#spNace").text(e.NaceAltiliKod);
                    $("#spNaceTanimi").text(e.NaceTanim);
                    $("#spMeslekKodu").text(e.MeslekKodu);
                    $("#spMeslekAdim").text(e.MeslekAdi);
                    $("#spTerkNedeni").text(e.MeslekTerkNedeniString);
                    $("#spMeslekAktarim").text(e.EskiMeslek);
                    $("#spAciklama").text(e.Aciklama);
                    $("#spKayitEden").text(e.KayitEden);

                    var terktarihi = e.MeslekTerkTar;
                    var kayittarihi = e.KayitTar;
                    var vizebitistarihi = e.VizeSuresiBitTar;
                    var sonvizeislemtrh = e.SonVizeIsTar;
                    var sondegisltrh = e.SonDegisTar;
                    var d, d1, d2, d3, d4;

                    if (sondegisltrh == null) {
                        $("#spSonDegTrh").text();
                    } else {
                        d4 = new Date(tarihformati(sondegisltrh));
                        $("#spSonDegTrh").text(d4.getDate() + "." + (d4.getMonth() + 1) + "." + d4.getFullYear());
                    }
                    if (sonvizeislemtrh == null) {
                        $("#spSonVizeIslemTrh").text();
                    } else {
                        d3 = new Date(tarihformati(sonvizeislemtrh));
                        $("#spSonVizeIslemTrh").text(d3.getDate() + "." + (d3.getMonth() + 1) + "." + d3.getFullYear());
                    }
                    if (vizebitistarihi == null) {
                        $("#spVizeBitis").text();
                    } else {
                        d2 = new Date(tarihformati(vizebitistarihi));
                        $("#spVizeBitis").text(d2.getDate() + "." + (d2.getMonth() + 1) + "." + d2.getFullYear());
                    }
                    if (kayittarihi == null) {
                        $("#spKayitTarihi").text();
                    } else {
                        d1 = new Date(tarihformati(kayittarihi));
                        $("#spKayitTarihi").text(d1.getDate() + "." + (d1.getMonth() + 1) + "." + d1.getFullYear());
                    }
                    if (terktarihi == null) {
                        $("#dvMeslek").css("background", "#e0ffd2");
                        $("#spTerkTarihi").text();
                    } else {
                        $("#dvMeslek").css("background", "#fce2e2");
                        d = new Date(tarihformati(terktarihi));
                        $("#spTerkTarihi").text(d.getDate() + "." + (d.getMonth() + 1) + "." + d.getFullYear());
                    }
                    $.ajax({
                        url: "/Sicil/SicilDetailMeslekLog",
                        type: "GET",
                        data: {
                            sicilno: @Model.Sicil.SICILNO,
                            sicilid: e.SicilId
                        },
                        dataType: "json",
                        headers: { "RequestVerificationToken": $('input[name="__RequestVerificationToken"]').val() },
                        success: function(liste) {
                            var trHtml = '';
                            trHtml += '<tr><th>İŞLEM TARİHİ</th><th>YAPILAN İŞLEM</th><th>İŞLEMİ YAPAN</th></tr>';
                            $.each(liste,
                                function(i, item) {
                                    var d = new Date(tarihformati(item.MakbuzTarihi));
                                    trHtml += '<tr><td>' + d.getDate() + "." + (d.getMonth() + 1) + "." + d.getFullYear() + " " + d.getHours() + ":" + d.getMinutes() + '</td><td>' + item.OdaAdi + '</td><td>' + item.Aciklama + '</td></tr>';
                                });
                            $('#tbMeslekLog').append(trHtml);
                        }
                    });

                });
        }

        function SicilMeslekDuzenle(id) {
            $.ErcanAyhan.Server.GetAsync("",
                "/Sicil/SicilMeslekDuzenle?id=" + id,
                function(t) {
                    // Select2 veri yüklemeleri...
                    $("[data-attribute='ID']").val(t.Id);
                    $("[data-attribute='SICILID']").val(t.SicilId);
                    $("[data-attribute='ISADRESILCE']").select2({
                        placeholder: 'İlçe seçiniz',
                        type: "GET",
                        data: t.Ilces
                    });
                    $("[data-attribute='ISADRESMAHALLE']").select2({
                        placeholder: 'Mahalle seçiniz',
                        type: "GET",
                        data: t.Mahalles
                    });
                    $("[data-attribute='ISADRESCADSOKBULV']").select2({
                        placeholder: 'Cadde/Sokak/Bulvar seçiniz',
                        type: "GET",
                        data: t.CadSokBulvs
                    });
                    $("[data-attribute='MESLEKODASI']").select2({
                        placeholder: 'Meslek Odası seçiniz',
                        type: "GET",
                        data: t.MeslekOdasis
                    });
                    $("[data-attribute='SINIF']").select2({
                        placeholder: 'Sınıf Odası seçiniz',
                        type: "GET",
                        data: t.Sinifs
                    });
                    $("[data-attribute='MESLEKTERKNEDENI']").select2({
                        placeholder: 'Terk Nedeni seçiniz',
                        type: "GET",
                        data: t.MeslekTerkNedenis
                    });
                    //Select2 value değerleri...
                    $("[data-attribute='ISADRESILCE']").select2("val", t.IsAdresIlce);
                    $("[data-attribute='ISADRESMAHALLE']").select2("val", t.IsAdresMahalle);
                    $("[data-attribute='ISADRESCADSOKBULV']").select2("val", t.IsAdresCadSokBulv);
                    $("[data-attribute='MESLEKODASI']").select2("val", t.MeslekOdasi);
                    $("[data-attribute='SINIF']").select2("val", t.Sinif);
                    $("[data-attribute='MESLEKTERKNEDENI']").select2("val", t.MeslekTerkNedeni);

                    $("[data-attribute='ISADRES']").val(t.IsAdres);
                    $("[data-attribute='ISADRES2']").val(t.IsAdres2);
                    $("[data-attribute='NACEKODU']").val(t.NaceId);
                    $("[data-attribute='ISYERIUNVANI']").val(t.IsyeriUnvani);
                    $("[data-attribute='MESLEK']").val(t.MeslekId);
                    $("[data-attribute='ACIKLAMA']").val(t.Aciklama);

                    $("#lblAdresTum").text(t.IsAdres2);
                    $("#nacekodu").val(t.NaceKodu);
                    $("#spNaceTanim").text(t.NaceTanimi);
                    $("#spMeslekKoduAdi").text(t.MeslekKodu + " - " + t.MeslekTanimi);

                    if (t.KayitTar != null) {
                        var d = new Date(tarihformati(t.KayitTar));
                        $("[data-attribute='KAYITTAR']").val(d.getDate() + "." + (d.getMonth() + 1) + "." + d.getFullYear());
                    }
                    if (t.SonDegisTar != null) {
                        var d2 = new Date(tarihformati(t.SonDegisTar));
                        $("[data-attribute='SONDEGISTAR']").val(d2.getDate() + "." + (d2.getMonth() + 1) + "." + d2.getFullYear());
                    }
                    if (t.SonVizeIsTar != null) {
                        var d3 = new Date(tarihformati(t.SonVizeIsTar));
                        $("[data-attribute='SONVIZEISTAR']").val(d3.getDate() + "." + (d3.getMonth() + 1) + "." + d3.getFullYear());
                    }
                    if (t.VizeSuresiBitTar != null) {
                        var d4 = new Date(tarihformati(t.VizeSuresiBitTar));
                        $("[data-attribute='VIZESURESIBITTAR']").val(d4.getDate() + "." + (d4.getMonth() + 1) + "." + d4.getFullYear());
                    }
                    if (t.MeslekTerkTar != null) {
                        var d5 = new Date(tarihformati(t.MeslekTerkTar));
                        $("[data-attribute='MESLEKTERKTAR']").val(d5.getDate() + "." + (d5.getMonth() + 1) + "." + d5.getFullYear());
                    }
                });
        }

        function tarihformati(d) {
            var tmp = d.match(/\d+/);
            if (tmp && tmp.length) {
                return parseInt(tmp[0]);
            }
            return 0;
        }

        function funcIlceChange() {
            var id = $("[data-attribute='ISADRESILCE']").val();
            $.ErcanAyhan.Server.GetAsync("",
                "/Sicil/GetirIlceyeGoreMahalle?id=" + id,
                function(e) {
                    $("[data-attribute='ISADRESMAHALLE']").select2({
                        type: "GET",
                        data: e,
                        placeholder: 'Mahalle seçiniz',
                    });
                });
            $("[data-attribute='ISADRESMAHALLE']").select2('open');
            $("[data-attribute='ISADRESMAHALLE']").val(null).trigger('change');
            $("[data-attribute='ISADRESCADSOKBULV']").val(null).trigger('change');
        }

        function funcMahalleChange() {
            var id = $("[data-attribute='ISADRESMAHALLE']").val();
            $.ErcanAyhan.Server.GetAsync("",
                "/Sicil/GetirMahalleyeGoreCadSokBulv?id=" + id,
                function(e) {
                    $("[data-attribute='ISADRESCADSOKBULV']").select2({
                        type: "GET",
                        data: e,
                        placeholder: 'Bulvar Cadde Sokak seçiniz',
                    });
                });
            $("[data-attribute='ISADRESCADSOKBULV']").select2('open');
        }

        function funcIlceChange2() {
            var id = $("[data-attribute='IsAdresIlce']").val();
            $.ErcanAyhan.Server.GetAsync("",
                "/Sicil/GetirIlceyeGoreMahalle?id=" + id,
                function(e) {
                    $("[data-attribute='IsAdresMahalle']").select2({
                        type: "GET",
                        data: e,
                        placeholder: 'Mahalle seçiniz',
                    });
                });
            $("[data-attribute='IsAdresMahalle']").select2('open');
            $("[data-attribute='IsAdresMahalle']").val(null).trigger('change');
            $("[data-attribute='IsAdresCadSokBulv']").val(null).trigger('change');
        }

        function funcMahalleChange2() {
            var id = $("[data-attribute='IsAdresMahalle']").val();
            $.ErcanAyhan.Server.GetAsync("",
                "/Sicil/GetirMahalleyeGoreCadSokBulv?id=" + id,
                function(e) {
                    $("[data-attribute='IsAdresCadSokBulv']").select2({
                        type: "GET",
